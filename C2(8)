string = input("Enter elements of a list separated by coma :")
list = string.split(",")
print('list: ', list)
max1 = len(list[0])
temp = list[0]
 
    # for loop to traverse the list
for i in list:
    if(len(i) > max1):
 
        max1 = len(i)
        temp = i
 
print("The word with the longest length is:", temp,
          " and length is ", max1)
